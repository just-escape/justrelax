  "conditions": {
    "button_is_being_pressed": [
      {
        "left": {"context": "from"},
        "operator": "=",
        "right": "button"
      },
      {
        "left": {"context": "message"},
        "operator": "=",
        "right": "press"
      }
    ],
    "from_load_cell": [
      {
        "left": {"context":  "from"},
        "operator": "=",
        "right": "load_cell"
      }
    ]
  },
  "rules": {
    "press_button": {
      "conditions": {"condition": "button_is_being_pressed"},
      "actions": {
        "action": [
          "switch_yellow_led_on",
          "cancel_red_and_green_led_hint_alarm"
        ]
      }
    },
    "load_cell": {
      "conditions": {"condition": "from_load_cell"},
      "actions": {
        "action": "forward_to_lights"
      }
    }
  },
  "alarms": {
    "red_and_green_led_hint_alarm": {
      "actions": {
        "action": [
          "switch_red_led_on",
          "switch_green_led_on"
        ]
      }
    }
  },
  "start_actions": {
    "action": [
      "start_red_and_green_led_hint_alarm"
    ]
  },
  "reset_actions": {
    "action": [
      "switch_red_led_off",
      "switch_green_led_off",
      "switch_yellow_led_off"
    ]
  }
